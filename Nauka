print(round(wynik1,2))
print("\n")

print("Zadanie 2")
lista20=[1,34,22,2,4,2,1,42]
print("najmniejszym elementem jest:",min(lista20))
print("Najmniejszy element powtarza sie:",lista20.count(min(lista20)))
print("\n")

print("Zadanie 4")
try:
    a=int(input("Podaj a:"))
    lista40=[1,2,3,4]
    lista41=[]
    for i in lista40:
        if a<i:
            lista41.append(i)
except ValueError:
    print("To nie jest liczba!")
    
    print(lista41)
print("liczb mniejszych jest:",len(lista41))
print("\n")

