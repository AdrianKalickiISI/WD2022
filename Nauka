print("Zadanie 2")
lista50 = [1, 2, 3, 'a', 'b', 'c']
def funkcja(listaliczb):

    listaliczb=[]
    listaznakow=[]
    for m in lista50:
        if type(m)==int:
            listaliczb.append(m)
        else:
            listaznakow.append(m)
    print(listaliczb)
    print(listaznakow)
    print("Liczb jest:", len(listaliczb))
    print("Znakow jest:", len(listaznakow))
funkcja(lista50)
print("\n")

print("Zadanie 3")
slownik30={1:'ogien',2:'woda','trzy':'3','cztery':'4',4.4:'to'}
slownik31=[]
print("Elementow jest:" ,len(slownik30))

for i in slownik30:
    if(type(i)==int):
        slownik31.append(slownik30[i])
print("Kluczy ktore sa liczbami jest:", len(slownik31))
print("\n")

